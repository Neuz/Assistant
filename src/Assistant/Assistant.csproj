<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\BeekeeperStudio.png" />
    <None Remove="Resources\BeekeeperStudio.svg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CliWrap" Version="3.4.4" />
    <PackageReference Include="CZGL.SystemInfo" Version="1.0.3" />
    <PackageReference Include="Microsoft.Toolkit.Mvvm" Version="7.1.2" />
    <PackageReference Include="MySqlBackup.NET.MySqlConnector" Version="2.3.6" />
    <PackageReference Include="Serilog.Sinks.RichTextBox.Wpf" Version="1.1.0" />
    <PackageReference Include="Syncfusion.Edit.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfAccordion.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfBusyIndicator.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfNavigationDrawer.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfProgressBar.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfSkinManager.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.SfTextInputLayout.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Themes.FluentDark.WPF" Version="20.2.0.36" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="20.2.0.36" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Img\BeekeeperStudio.png">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Img\BeekeeperStudio.svg">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="View\ToolsView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="View\SystemInfoView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="View\WizardControl\MySQLWizardView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="View\WizardControl\NeuzAdapterK3WiseWizardView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="View\WizardControl\NeuzAppWizardView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="View\WizardControl\NeuzAdapterKisWizardView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="View\WizardControl\RedisWizardView.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="View\ToolsView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\SystemInfoView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\WizardControl\MySQLWizardView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\WizardControl\NeuzAdapterK3WiseWizardView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\WizardControl\NeuzAppWizardView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\WizardControl\NeuzAdapterKisWizardView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\WizardControl\RedisWizardView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
